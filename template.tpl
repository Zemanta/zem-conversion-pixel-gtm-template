const injectScript = require('injectScript');
const setInWindow = require('setInWindow');
const createArgumentsQueue = require('createArgumentsQueue');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');

//check if api already exists
let api = copyFromWindow('zemApi');

if (!api) {
  const url = 'https://js-tag.zemanta.com/zcpt.js';
  let zemTag;
  api = function () {
   	zemTag(arguments[0], arguments[1], arguments[2]);
  };

  api.version = '1.0-gtm';
  api.loaded = true;
  api.marketerId = data.MarketerId;
  setInWindow('zemApi', api, true);
  zemTag = createArgumentsQueue('zemTag', 'zemApi.queue');
  api('track', 'PAGE_VIEW');
  injectScript(url, data.gtmOnSuccess, data.gtmOnFailure);
} else {
  callInWindow('zemApi.addMarketer', data.MarketerId);
}


